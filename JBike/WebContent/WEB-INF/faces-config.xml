<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<lifecycle>
		<phase-listener>com.jbike.listener.MessageListener</phase-listener>
	</lifecycle>
	<navigation-rule>
		<from-view-id>/admin/*</from-view-id>
		<navigation-case>
			<from-outcome>bikes/form</from-outcome>
			<to-view-id>/admin/bikes/form.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>bikes/history</from-outcome>
			<to-view-id>/admin/bikes/history.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>bikes/list</from-outcome>
			<to-view-id>/admin/bikes/list.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>stations/form</from-outcome>
			<to-view-id>/admin/stations/form.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>stations/information</from-outcome>
			<to-view-id>/admin/stations/information.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>stations/list</from-outcome>
			<to-view-id>/admin/stations/list.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>users/form</from-outcome>
			<to-view-id>/admin/users/form.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>users/list</from-outcome>
			<to-view-id>/admin/users/list.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>users/profile</from-outcome>
			<to-view-id>/admin/users/profile.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/user/*</from-view-id>
		<navigation-case>
			<from-outcome>movements/list</from-outcome>
			<to-view-id>/user/movements/list.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/index.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>login</from-outcome>
			<to-view-id>/users/log-in.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>signin</from-outcome>
			<to-view-id>/users/sign-in.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>myprofile</from-outcome>
			<to-view-id>/users/my-profile.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>bikes/list</from-outcome>
			<to-view-id>/bikes/list.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
		<navigation-case>
			<from-outcome>user/movements</from-outcome>
			<to-view-id>/users/movements.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>
</faces-config>
